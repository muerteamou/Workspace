String cadenaSQL = "SELECT * FROM Socio"; // o llamar a un procedimiento
if (!txtbuscar.getText().isEmpty())
	cadenaSQL = cadenaSQL + "WHERE localidad = '" + txtBuscar.getText() + "'";
	listado = s.executeQuery (cadenaSQL);
	listado.last();
	filas = listado.getRow();
	
